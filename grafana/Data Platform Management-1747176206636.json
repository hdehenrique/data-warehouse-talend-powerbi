          "editorMode": "code",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT DATE_FORMAT(t.date_creation, \"%d/%m/%Y %H:%i\") AS \"Created Date\",\r\nt.id AS \"Nº Ticket\",\r\nt.name AS \"Descrição Ticket\",\r\nCONCAT(u.firstname, ' ', u.realname) AS \"Requester\",\r\nCONCAT(u2.firstname, ' ', u2.realname) AS \"Responsible\",\r\nCASE \r\n   WHEN t.status = 1 THEN 'Backlog'\r\n   WHEN t.status = 2 THEN 'Open Assigned'\r\n   WHEN t.status = 3 THEN 'Open Assigned'\r\n   WHEN t.status = 4 THEN 'Pending'\r\n   WHEN t.status = 7 THEN 'Accepted'\r\n   WHEN t.status = 8 THEN 'Observe'\r\n   WHEN t.status = 9 THEN 'Evaluation'\r\n   WHEN t.status = 10 THEN 'Approved'\r\n   WHEN t.status = 11 THEN 'Test'\r\n   WHEN t.status = 12 THEN 'Qualified'\r\nEND AS \"Status\",\r\nCASE \r\n   WHEN t.priority = 1 THEN 'Very Low'\r\n   WHEN t.priority = 2 THEN 'Low'\r\n   WHEN t.priority = 3 THEN 'Average'\r\n   WHEN t.priority = 4 THEN 'High'\r\n   WHEN t.priority = 5 THEN 'Very High'\r\nEND AS \"Priority\"\r\nFROM glpi_tickets t\r\nLEFT OUTER JOIN glpi_entities e ON t.entities_id = e.id\r\nINNER JOIN glpi_groups_tickets ggt ON ggt.tickets_id = t.id\r\nLEFT OUTER JOIN glpi_users u ON u.id = (SELECT tu1.users_id FROM glpi_tickets_users tu1 WHERE tu1.tickets_id = t.id AND tu1.`type` = 1 LIMIT 1)\r\nLEFT OUTER JOIN glpi_users u2 ON u2.id = (SELECT tu2.users_id FROM glpi_tickets_users tu2 WHERE tu2.tickets_id = t.id AND tu2.`type` = 2 LIMIT 1)\r\nWHERE t.status <> '5'\r\n  AND t.is_deleted = 0\r\n  and t.status <> '6'\r\n  and t.time_to_resolve is not null\r\n  and t.id not in (3185,3508, 5092,3179,3178)\r\n  and ggt.groups_id = '63'  \r\nORDER BY t.date_creation DESC",
